                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }
            },
            "example": {
                "data": [
                    [
                        "1",
                        "Thursday",
                        "16",
                        "1",
                        "3.48"
                    ]
                ]
            }
        },
        "ServiceOutput": {
            "type": "array",
            "items": {
                "type": "number",
                "format": "double"
            },
            "example": [
                13.66304196
            ]
        },
        "ErrorResponse": {
